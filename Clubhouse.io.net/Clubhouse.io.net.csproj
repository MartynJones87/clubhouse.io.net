<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.0.4-alpha</Version>
    <Description>A .Net client library for the Clubhouse API https://clubhouse.io/api</Description>
    <Authors>Martyn Jones</Authors>
    <Company>Martyn Jones</Company>
    <Copyright>Copyright 2017</Copyright>
    <PackageLicenseUrl>https://raw.githubusercontent.com/MartynJones87/clubhouse.io.net/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/MartynJones87/clubhouse.io.net</PackageProjectUrl>
    <PackageTags>Clubhouse REST API</PackageTags>
    <PackageReleaseNotes>Fixing Story Create Bug
Refactoring Clubhouse class name to avoid clash between Namespace and Class
Ensuring parameters required by the Clubhouse API to create story are populated before the API call is made.</PackageReleaseNotes>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Refit" Version="4.0.1" />
  </ItemGroup>

</Project>
